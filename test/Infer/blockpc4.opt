

; RUN: %souper-check -infer-rhs -souper-only-infer-iN %s > %t
; RUN: %FileCheck %s -check-prefix=SUCCESS < %t

; SUCCESS: result 3:i32

%0 = block 4
%1:i32 = var
%2:i32 = urem %1, 4:i32
%3:i1 = ne 0:i32, %2
blockpc %0 0 %3 1:i1
%4:i1 = ne 1:i32, %2
blockpc %0 0 %4 1:i1
%5:i1 = ne 2:i32, %2
blockpc %0 0 %5 1:i1
blockpc %0 1 %2 2:i32
blockpc %0 2 %2 1:i32
blockpc %0 3 %2 0:i32
%6:i32 = add 1:i32, %1
%7:i32 = add 2:i32, %1
%8:i32 = add 3:i32, %1
%9:i32 = phi %0, %1, %6, %7, %8
%10:i32 = and 3:i32, %9
infer %10
