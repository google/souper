; REQUIRES: solver
; RUN: %souper-check -infer-rhs -souper-infer-nop -souper-stress-nop %solver %s > %t1
; RUN: %souper-check -infer-rhs -souper-exhaustive-synthesis %solver %s > %t2
; RUN: %FileCheck %s < %t1
; RUN: %FileCheck %s < %t2
; CHECK: result %a

%a:i32 = var
%b:i32 = var
%0 = add %a, %b
%1 = sub %0, %b
infer %1
