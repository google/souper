; REQUIRES: solver
; RUN: %souper-check %solver %s | %FileCheck %s
; CHECK: LGTM

; !((v & 0xff) && (v & 0xff00) && (v & 0xff0000) && (v & 0xff000000))

%0:i32 = var
%1:i32 = and %0, 255
%2:i32 = and %0, 65280
%3:i32 = and %0, 16711680
%4:i32 = and %0, -16777216
%5:i1 = ne %1, 0
%6:i1 = ne %2, 0
%7:i1 = ne %3, 0
%8:i1 = ne %4, 0
%9:i1 = and %5, %6
%10:i1 = and %7, %8
%11:i1 = and %9, %10
%12:i1 = xor %11, 1
infer %12

; !!((v - 0x01010101UL) & ~v & 0x80808080UL)

%100:i32 = sub %0, 16843009
%101:i32 = xor %0, -1
%102:i32 = and %101, 2155905152
%103:i32 = and %100, %102
%104:i1 = ne %103, 0
result %104
