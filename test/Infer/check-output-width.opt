; REQUIRES: solver, solver-model

; RUN: %souper-check %solver -souper-infer-iN=false -infer-rhs -souper-infer-inst -souper-synthesis-comps=const %s > %t1
; RUN: %souper-check %solver -souper-infer-iN=false -infer-rhs -souper-enumerative-synthesis %s > %t2
; RUN: %FileCheck %s < %t1
; RUN: %FileCheck %s < %t2

; CHECK: Failed to infer RHS

%0:i64 = var
%1:i64 = addnw 1:i64, %0
%2:i32 = trunc %1
infer %2
