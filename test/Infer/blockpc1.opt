; REQUIRES: solver, solver-model

; RUN: %souper-check %solver -infer-rhs -souper-infer-iN %s > %t
; RUN: %FileCheck %s -check-prefix=SUCCESS < %t

; SUCCESS: result 10:i32

%0 = block 2
%1:i32 = var
%2:i1 = eq 0:i32, %1
blockpc %0 0 %2 0:i1
blockpc %0 1 %2 1:i1
%3:i32 = phi %0, 4:i32, 7:i32
%4:i32 = phi %0, 6:i32, 3:i32
%5:i32 = addnw %3, %4
infer %5
