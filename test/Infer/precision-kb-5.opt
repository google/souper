; REQUIRES: solver
; RUN: %souper-check %solver -infer-rhs -infer-known-bits %s

; CHECK: knownBits from souper: 00000000000000xxxxxxxxxxxxxxxxxx

%0:i16 = var
%1:i32 = zext %0
%2:i32 = addnw 2:i32, %1
%3:i16 = var
%4:i32 = zext %3
%5:i32 = shlnw %4, 1:i32
%6:i32 = addnw %2, %5
%7:i16 = var
%8:i32 = zext %7
%9:i32 = addnw %6, %8
infer %9
