; REQUIRES: solver, solver-model, synthesis

; RUN: %souper-check %solver -infer-rhs -souper-infer-inst -souper-synthesis-comps=sub,const,or,lshr,xor %s > %t1
; RUN: FileCheck %s < %t1

; CHECK:      %6:i32 = or %1, %3
; CHECK-NEXT: %7:i32 = sub %6, %4
; CHECK-NEXT: result %7

; Ceil of average of two unsigned integers without over-flowing
%0:i32 = var
%1:i32 = var
%2:i32 = or %0, %1
%3:i32 = xor %0, %1
%4:i32 = lshr %3, 1
%5:i32 = sub %2, %4
infer %5
