; REQUIRES: synthesis

; RUN: %souper-check -infer-rhs -souper-infer-inst -souper-synthesis-ignore-cost -souper-synthesis-comps=ult,and,xor,const %s > %t1
; RUN: %FileCheck %s < %t1

; CHECK:      %5:i32 = {{(xor|and)}} %0, %1
; CHECK-NEXT: %6:i32 = {{(and|xor)}} %0, %5
; CHECK-NEXT: %7:i1 = ult %1, %6
; CHECK-NEXT: result %7

; Test if nlz(x) < nlz(y) where nlz is number of leading zeros
%0:i32 = var
%1:i32 = var
%2:i32 = xor %1, -1
%3:i32 = and %0, %2
%4:i1 = ult %1, %3
infer %4
