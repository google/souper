; REQUIRES: solver, solver-model

; Exhaustive synthesis does not support this test, since result has more than
; one instruction
;
; RUN: %souper-check %solver -infer-rhs -souper-infer-inst -souper-synthesis-ignore-cost -souper-synthesis-comps=sub %s > %t1
; RUN: %FileCheck %s < %t1

; CHECK:      %5:i32 = sub %3, %1
; CHECK-NEXT: result %5

%0:i1 = var
%1:i32 = zext %0
%2:i8 = var
%3:i32 = zext %2
%4:i32 = subnsw %3, %1
infer %4
