; REQUIRES: solver, synthesis
; RUN: %souper-check -infer-rhs -souper-exhaustive-synthesis -souper-exhaustive-synthesis-num-instructions=2 %solver %s > %t1
; RUN: %FileCheck %s < %t1

; synthesis two constants
%0:i16 = var
%1:i16 = add %0, 5:i16
%2:i16 = add %1, 7:i16
%3:i16 = mul %2, 32:i16
infer %3
; result can be (x + 12) << 5 or x * 32 + 384
; CHECK: %4:i16 = {{add 12:i16, %0|mul 32:i16, %0}}
; CHECK: %5:i16 = {{mul 32:i16, %4|shl %4, 5:i16|add 384:i16, %4}}
