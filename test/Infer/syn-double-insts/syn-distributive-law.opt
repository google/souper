; REQUIRES: solver, synthesis, long-duration-synthesis
; RUN: %souper-check -infer-rhs -souper-exhaustive-synthesis -souper-exhaustive-synthesis-num-instructions=2 %solver %s > %t1
; RUN: %FileCheck %s < %t1

; synthesis add/mul (distributive law)
%0:i4 = var
%1:i4 = var
%2:i4 = var
%3:i4 = mul %0, %2
%4:i4 = mul %1, %2
%5:i4 = add %3, %4
infer %5
; CHECK: %6:i4 = add %0, %1
; CHECK: %7:i4 = mul %2, %6
