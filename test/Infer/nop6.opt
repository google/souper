; REQUIRES: solver
; RUN: %souper-check -infer-rhs -souper-infer-nop %solver %s > %t1
; RUN: %souper-check -infer-rhs -souper-exhaustive-synthesis %solver %s > %t2
; RUN: %FileCheck %s < %t1
; RUN: %FileCheck %s < %t2

; CHECK: result %c

%a:i32 = var
%b:i32 = var
%c:i32 = add %a, 3
%0 = add %c, %b
%1 = sub %0, %b
infer %1
