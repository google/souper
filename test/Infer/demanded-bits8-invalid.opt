; REQUIRES: solver, solver-model

; RUN: %souper-check %solver -infer-rhs -souper-infer-iN %s > %t 2>&1
; RUN: %FileCheck %s < %t

; CHECK: Failed to infer RHS

%0 = block 2
%1:i32 = var
%2:i32 = and 268435455:i32, %1
%3:i64 = zext %2
%4:i32 = var (nonNegative) (signBits=4)
%5:i64 = sext %4
%6:i64 = phi %0, %3, %5
infer %6
