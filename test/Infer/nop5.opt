; REQUIRES: solver
; RUN: %souper-check -infer-rhs -souper-infer-nop %solver %s > %t1
; RUN: %souper-check -infer-rhs -souper-enumerative-synthesis %solver %s > %t2
; RUN: %FileCheck %s < %t1
; RUN: %FileCheck %s < %t2
; CHECK: Failed to infer RHS

%0:i64 = var
%1 = eq -1, %0
%2:i64 = var
%3 = eq %2, %0
%4 = or %1, %3
infer %4
