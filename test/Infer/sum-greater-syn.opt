; REQUIRES: solver, solver-model

; RUN: %souper-check %solver -infer-rhs -souper-infer-inst -souper-synthesis-comps=const,slt %s > %t1
; RUN: %FileCheck %s < %t1

; return b > 0
; CHECK:      %5:i1 = slt 0:i32, %1
; CHECK-NEXT: %6:i32 = zext %5
; CHECK-NEXT: result %6

; return a + b > 0
%0:i32 = var
%1:i32 = var
%2:i32 = addnsw %0, %1
%3:i1 = slt %0, %2
%4:i32 = zext %3
infer %4
