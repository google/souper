; REQUIRES: solver, solver-model

; RUN: %souper-check %solver -infer-rhs -souper-infer-iN %s > %t
; RUN: FileCheck %s -check-prefix=SUCCESS < %t

; SUCCESS: result 0:i32

%0 = block 2
%1:i32 = var
%2:i32 = srem %1, 2:i32
%3:i1 = ne 0:i32, %2
blockpc %0 0 %3 1:i1
blockpc %0 1 %3 0:i1
%4:i32 = addnsw 1:i32, %1
%5:i32 = phi %0, %4, %1
%6:i32 = and 1:i32, %5
infer %6
