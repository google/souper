; REQUIRES: synthesis

; RUN: %souper-check -try-dataflow-pruning %s > %t
; RUN: %FileCheck %s < %t

; CHECK: Pruning succeeded

%0 = block 1
%1:i32 = var
%2:i32 = add 0:i32, %1
%3:i32 = phi %0, %2
infer %3
%4:i32 = reservedconst
%5:i32 = add %1, %4
result %5


; CHECK: Pruning failed

%0 = block 1
%1:i32 = var
%2:i32 = add 1:i32, %1
%3:i32 = phi %0, %2
infer %3
%4:i32 = reservedconst
%5:i32 = add %1, %4
result %5

