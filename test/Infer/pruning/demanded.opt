; REQUIRES: synthesis

; RUN: %souper-check -try-dataflow-pruning %s > %t 2>&1
; RUN: %FileCheck %s < %t

; CHECK: pruned using demanded bits analysis.
%0:i64 = var
%1:i64 = var
%2:i64 = and %0, %1
infer %2
%3:i64 = reservedconst
%4:i64 = add %0, %3
result %4

; CHECK: pruned using demanded bits analysis.
%0:i32 = var
%1:i32 = var
%2 = and %1, 5
%3 = or %2, 42
%4 = add %3, 0
%5 = sub %4, %0
infer %5
%6:i32 = reservedconst
%7:i32 = sub %1, %6
result %7
