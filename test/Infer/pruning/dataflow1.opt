; REQUIRES: solver, synthesis

; RUN: %souper-check %solver -try-dataflow-pruning %s > %t 2>&1
; RUN: %FileCheck %s < %t

; CHECK: pruned using concrete interpreter!
%0:i32 = var
%1 = add %0, 1
%2 = and %1, 5
%3 = or %2, 42
%4 = add %3, 0
infer %4
%5:i32 = add %0, %0
result %5

; CHECK: pruned using restricted bits analysis.
%0:i32 = var
%1 = add %0, 1
%2 = and %1, 5
%3 = or %2, 42
%4 = add %3, 0
infer %4
%5:i32 = reservedconst
%6:i32 = add %0, %5
result %6

; CHECK: pruned using CR! Inst had a symbolic const.
%0:i32 = var ; 0
%1:i32 = add 1:i32, %0
%2:i32 = and 5:i32, %1
%3:i32 = or 42:i32, %2
%4:i32 = add 0:i32, %3
infer %4
%5:i32 = reservedconst
%6:i32 = lshr %5, %0
result %6

; CHECK: pruned using KB! Inst had a symbolic const.
%0:i32 = var ; 0
%1:i32 = add 1:i32, %0
%2:i32 = and 5:i32, %1
%3:i32 = or 42:i32, %2
%4:i32 = add 0:i32, %3
infer %4
%5:i32 = reservedconst
%6:i32 = fshl %0, %0, %5
%7:i32 = freeze %6
result %7
