; RUN: %souper-check -souper-use-alive %s > %t 2>&1
; RUN: %FileCheck %s < %t

%0 = block 2
%1:i32 = var
%2:i32 = var
%3:i32 = phi %0, %1, %1
infer %3
result %1

; CHECK: LGTM

%0 = block 2
%1:i32 = var
%2:i32 = var
%3:i32 = phi %0, %1, %2
infer %3
result %1

; CHECK: Invalid

%0 = block 2
%1:i32 = var
%2:i32 = var
%3:i32 = phi %0, %1, %1
%4:i32 = phi %0, %2, %2
%5:i32 = add %3, %4
infer %5
%6:i32 = add %1, %2
result %6

; CHECK: LGTM


%0 = block 2
%1:i32 = var
%2:i32 = var
%3:i32 = phi %0, %1, %1
%4:i32 = phi %0, %3, %1
infer %4
result %1

; CHECK: LGTM

%0 = block 2
%1:i32 = var
%2:i32 = var
%3:i32 = phi %0, %1, %2
%4:i32 = phi %0, %3, %1
infer %4
result %1

; CHECK: LGTM

%0 = block 2
%1:i32 = var
%2:i32 = var
%3:i32 = phi %0, %2, %1
%4:i32 = phi %0, %3, %1
infer %4
result %1

; CHECK: Invalid


%0 = block 2
%1 = block 2
%2:i32 = var
%3:i32 = var
%4:i32 = phi %0, %2, %3
%5:i32 = phi %1, %4, %2
infer %5
result %2

; CHECK: Invalid

%0:i32 = var
%1 = block 2
%2 = block 2
%3 = block 2
%4:i32 = var
%5:i32 = or %0, %4
%6:i32 = phi %3, %5, 0:i32
%7:i32 = or %0, %6
%8:i32 = phi %2, %7, 0:i32
%9:i32 = or %0, %8
%10:i32 = phi %1, %9, 0:i32
%11:i32 = or %0, %10
infer %11
result %9

; CHECK: Invalid
