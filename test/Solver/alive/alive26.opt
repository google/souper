; Name: 735
; Pre: WillNotOverflowUnsignedMul(C1, C2)
; %Op0 = udiv i7 %X, C1
; %r = udiv %Op0, C2
;   =>
; %r = udiv %X, C1*C2

%X:i8 = var
%Op0 = udiv %X, 13
%r = udiv %Op0, 14
infer %r
%r2 = udiv %X, 182
result %r2

; Name: 748
; Pre: C2 % C1 == 0
; %m = mul nsw i7 %X, C1
; %r = sdiv %m, C2
;   =>
; %r = sdiv %X, C2/C1

%X:i8 = var
%m = mulnsw %X, 9
%r = sdiv %m, 81
infer %r
%r2 = sdiv %X, 9
result %r2

; Name: 748
; Pre: C2 % C1 == 0
; %m = mul nsw i7 %X, C1
; %r = sdiv exact %m, C2
;   =>
; %r = sdiv exact %X, C2/C1

%X:i8 = var
%m = mulnsw %X, 25
%r = sdivexact %m, 100
infer %r
%r2 = sdivexact %X, 4
result %r2

; Name: 748
; Pre: C2 %u C1 == 0
; %m = mul nuw i7 %X, C1
; %r = udiv %m, C2
;   =>
; %r = udiv %X, C2 /u C1

%X:i16 = var
%m = mulnuw %X, 100
%r = udiv %m, 15000
infer %r
%r2 = udiv %X, 150
result %r2

; Name: 748
; Pre: C2 %u C1 == 0
