; Name: AndOrXor:923
; Pre: C1 u< C2
; %op0 = icmp eq %a, C1
; %op1 = icmp ult %a, C2
; %r = and %op0, %op1
;   =>
; %r = %op0

%a:i32 = var
%op0 = eq %a, 3452
%op1 = ult %a, 12435
%r = and %op0, %op1
infer %r
result %op0

; Name: AndOrXor:924
; Pre: C1 < C2
; %op0 = icmp eq %a, C1
; %op1 = icmp slt %a, C2
; %r = and %op0, %op1
;   =>
; %r = %op0

%a:i16 = var
%op0 = eq %a, -5323
%op1 = slt %a, 633
%r = and %op0, %op1
infer %r
result %op0

; Name: AndOrXor:931
; Pre: C1 u< C2 && C1 == C2-1
; %op0 = icmp ne %a, C1
; %op1 = icmp ult %a, C2
; %r = and %op0, %op1
;   =>
; %r = icmp ult %a, C1

%a:i32 = var
%op0 = ne %a, 62562
%op1 = ult %a, 62563
%r = and %op0, %op1
infer %r
%r2 = ult %a, 62562
result %r2

; Name: AndOrXor:937
