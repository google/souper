; %op0 = icmp ult %a, C1
; %op1 = icmp eq %a, C2
; %r = and %op0, %op1
;   =>
; %r = false

%a:i32 = var
%op0 = ult %a, 24562456
%op1 = eq %a, -2
%r = and %op0, %op1
infer %r
result 0:i1

