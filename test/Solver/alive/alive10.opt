; Name: AndOrXor:949
; Pre: C1 u< C2 && C1 == C2-1
; %op0 = icmp ne %a, C1
; %op1 = icmp ne %a, C2
; %r = and %op0, %op1
;   =>
; %s = add %a, -C1
; %r = icmp ugt %s, 1

%a:i32 = var
%op0 = ne %a, 34529346
%op1 = ne %a, 34529347
%r = and %op0, %op1
infer %r
%s = add %a, -34529346
%r2 = ult 1, %s
result %r2

; Name: AndOrXor:961
