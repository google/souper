; Name: 266-2
; %div = sdiv exact %X, %Y
; %negY = sub 0, %Y
; %r = mul %div, %negY
;   =>
; %r = sub 0, %X

%X:i16 = var
%Y:i16 = var
%div = sdivexact %X, %Y
%negY = sub 0, %Y
%r = mul %div, %negY
infer %r
%r2 = sub 0, %X
result %r2

; Name: 275
; %div = udiv i5 %X, %Y
; %r = mul %div, %Y
;   =>
; %rem = urem %X, %Y
; %r = sub  %X, %rem

%X:i8 = var
%Y:i8 = var
%div = udiv %X, %Y
%r = mul %div, %Y
infer %r
%rem = urem %X, %Y
%r2 = sub %X, %rem
result %r2

