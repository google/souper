; Name: AndOrXor:1829
; Pre: C1 < C2
; %cmp1 = icmp ne %X, C1
; %cmp2 = icmp sgt %X, C2
; %r = or %cmp1, %cmp2
;   =>
; %r = %cmp1

%X:i32 = var
%cmp1 = ne %X, -436234
%cmp2 = slt 7489399, %X
%r = or %cmp1, %cmp2
infer %r
result %cmp1

; Name: AndOrXor:1831
