; Name: AndOrXor:966
; Pre: C1 u< C2
; %op0 = icmp ult %a, C1
; %op1 = icmp ne %a, C2
; %r = and %op0, %op1
;   =>
; %r = %op0

%a:i32 = var
%op0 = ult %a, 3346
%op1 = ne %a, 3347
%r = and %op0, %op1
infer %r
result %op0

