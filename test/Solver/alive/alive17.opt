; Name: AndOrXor:1880
; Pre: C1 u< C2
; %cmp1 = icmp ugt %X, C1
; %cmp2 = icmp ugt %X, C2
; %r = or %cmp1, %cmp2
;   =>
; %r = %cmp1

%X:i16 = var
%cmp1 = ult 63200, %X
%cmp2 = ult 64001, %X
%r = or %cmp1, %cmp2
infer %r
result %cmp1

; Name: AndOrXor:1884
; Pre: C1 u< C2
; %cmp1 = icmp ugt %X, C1
; %cmp2 = icmp ne %X, C2
; %r = or %cmp1, %cmp2
;   =>
; %r = true

%X:i32 = var
%cmp1 = ult 234532, %X
%cmp2 = ne %X, 4564554
%r = or %cmp1, %cmp2
infer %r
result 1:i1

