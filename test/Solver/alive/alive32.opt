; %c = icmp slt %x, C
; %X = sext %x to i15
; %r = select %c, %X, C2
;   =>
; %c2 = icmp sgt %X, C2
; %r = select %c2, C2, %X

%x:i16 = var
%c = slt %x, 5555
%X:i32 = sext %x
%r = select %c, %X, 5554
infer %r
%c2 = slt 5554, %X
%r2 = select %c2, 5554, %X
result %r2

; Name: Select:525
