; Name: Select:523
; Pre: (C2 == sext(C+1)) && C != (1<<(width(%x)-1))-1
; %c = icmp sgt %x, C
; %X = sext %x to i15
; %r = select %c, %X, C2
;   =>
; %c2 = icmp slt %X, C2
; %r = select %c2, C2, %X

%x:i8 = var
%c = slt -125, %x
%X:i32 = sext %x
%r = select %c, %X, -124
infer %r
%c2 = slt %X, -124
%r2 = select %c2, -124, %X
result %r2

; Name: Select:524
