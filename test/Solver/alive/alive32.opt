; Name: Select:575
; %c = icmp sgt %X, -1
; %r = select %c, C1, C2
;   =>
; %s = ashr %X, width(%X)-1
; %a = and %s, C2-C1
; %r = add %a, C1

%X:i64 = var
%c = slt -1, %X
%r = select %c, -8494242232709579858:i64, -3671342357661587618
infer %r
%s = ashr %X, 63
%a = and %s, 4822899875047992240
%r2 = add %a, -8494242232709579858
result %r2

; Name: Select:575
; %c = icmp sgt %X, -1
; %r = select %c, C1, -1
;   =>
; %s = ashr %X, width(%X)-1
; %r = or %s, C1

%X:i32 = var
%c = slt -1, %X
%r = select %c, 986256027, -1:i32
infer %r
%s = ashr %X, 31
%r2 = or %s, 986256027
result %r2

