; Name: AndOrXor:2247  (~A | ~B) == (~(A & B))
; %na = xor %A, -1
; %nb = xor %B, -1
; %r = or %na, %nb
;   =>
; %a = and %A, %B
; %r = xor %a, -1

%A:i32 = var
%B:i32 = var
%na = xor %A, -1
%nb = xor %B, -1
%r = or %na, %nb
infer %r
%a = and %A, %B
%r2 = xor %a, -1
result %r2

; Name: AndOrXor:2263
; %op1 = xor %op0, %B
; %r = or %op0, %op1
;   =>
; %r = or %op0, %B

%op0:i64 = var
%B:i64 = var
%op1 = xor %op0, %B
%r = or %op0, %op1
infer %r
%r2 = or %op0, %B
result %r2

