%Z:i32 = var
%RHS:i32 = var
%Y = or %Z, 4042322160 ; 0xf0f0f0f0
%X = xor %Y, 252645135 ; 0x0f0f0f0f
%LHS = add %X, 1
%r = add %LHS, %RHS
infer %r
%and = and %Z, 252645135
%r2 = sub %RHS, %and
result %r2

%Z:i64 = var
