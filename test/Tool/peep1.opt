; REQUIRES: solver

; RUN: %souper-check %solver -print-replacement %s > %t1
; RUN: %FileCheck %s < %t1
; RUN: %parser-test < %t1
; RUN: %souper-check %solver %t1 | %FileCheck %s

; RUN: %souper-check %solver -print-replacement-split %s > %t2
; RUN: %FileCheck %s < %t2
; RUN: %parser-test < %t2
; RUN: %souper-check %solver %t2 | %FileCheck %s

; CHECK: LGTM

%0:i32 = var
%1:i32 = var
%2:i32 = xor %1, -1
%3:i32 = or %0, %2
%4:i32 = xor %3, -1
%5:i32 = xor %0, -1
%6:i32 = and %5, %1
cand %4 %6
