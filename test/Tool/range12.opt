; REQUIRES: solver

; RUN: %souper-check %solver -print-counterexample=false  %s > %t 2>&1
; RUN: %FileCheck %s < %t

; CHECK: LGTM
%0:i8 = var (knownBits=000000xx) (nonNegative) (nonZero) (signBits=6) (range=[1,3))
%1:i8 = var (knownBits=00000xxx) (nonNegative) (nonZero) (signBits=5) (range=[1,5))
%2:i8 = shl %0, %1
%3:i1 = ule %2, 32:i8
cand %3 1:i1
