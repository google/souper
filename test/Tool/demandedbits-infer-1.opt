; REQUIRES: solver, solver-model

; RUN: %souper-check %solver -infer-rhs -print-replacement -souper-infer-iN %s > %t 2>&1
; RUN: %FileCheck %s < %t

; CHECK: %0:i64 = var
; CHECK-NEXT: %1:i64 = lshr %0, 2:i64 
