

; RUN: %souper-check -print-counterexample=false %s > %t 2>&1
; RUN: %FileCheck %s < %t

; CHECK: Invalid
%0 = block 3
%1:i32 = var
%2:i1 = ne 0:i32, %1
%3:i1 = ne 1:i32, %1
blockpc %0 1 %1 0:i32
blockpc %0 2 %1 0:i32
%4:i32 = addnsw 9:i32, %1
%5:i32 = addnsw 10:i32, %1
%6:i32 = phi %0, 10:i32, %4, %5
%7:i1 = eq 10:i32, %6
cand %7 1:i1
